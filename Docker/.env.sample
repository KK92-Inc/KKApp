# Docker images
# ============================================================================
# Image names/tags for each service. Leave empty to use CI/default tag.
BACKEND_IMAGE=
FRONTEND_IMAGE=
GIT_API_IMAGE=
GIT_SSH_IMAGE=
MIGRATION_JOB_IMAGE=

# Ports
# ============================================================================
# Port exposed by the backend service (host or container port as required).
BACKEND_PORT=

# Origins / front-end
# Base URL/origin for the front-end (e.g. https://app.example.com)
FE_ORIGIN=

# Database
# Password for the primary database user. Keep secret.
DATABASE_PASSWORD=

# Keycloak
# Keycloak realm name and client credentials
KC_REALM=
KC_ID=       # client id
KC_SECRET=   # client secret (keep secret)
KC_COOKIE=   # cookie key/name used by Keycloak sessions
KC_ORIGIN=   # base URL of Keycloak (e.g. https://auth.example.com)
# Bind mount path for importing Keycloak realm config from host (optional)
KEYCLOAK_BINDMOUNT_0=

# S3 / Object Storage
# Credentials for S3-compatible storage used for uploads/backups
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=

# Other secrets
# Password for envelope/key encryption or other vaulted secret
VALKEY_PASSWORD=
